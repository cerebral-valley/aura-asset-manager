{
  "buildCommand": "cd frontend && pnpm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && pnpm install",
  "rewrites": [
    {
      "source": "/assets",
      "destination": "/assets"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/targets",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api)(?!_next)(?!static)(?!assets).*)",
      "destination": "/index.html"
    }
  ]
}
